    <link rel="apple-touch-icon" sizes="57x57" href="/ozkinaci/tema/images/favicon/apple-icon-57x57.png">
    <link rel="apple-touch-icon" sizes="60x60" href="/ozkinaci/tema/images/favicon/apple-icon-60x60.png">
